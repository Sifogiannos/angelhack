<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name = "viewport"  content="minimal-ui width=device-width, user-scalable=no, initial-scale=1.0,maximum-scale=1.0">
    <meta name = "msapplication-tap-highlight" content="no">
    <title>Stranded | Dashboard</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="/css/main-theme.css">
    <link rel="stylesheet" type="text/css" href="/css/side-menu.css">
    <link rel="stylesheet" type="text/css" href="/css/dashboard.css">
  </head>

  <body>
  	<section class="background-image">
      <div class="overlay"></div>
      <div class="menu-wrap">
        <nav class="menu">
          <div class="icon-list">
            <h2>Stranded</h2>
            <a href="#" id="agendaLink"><i class="fa fa-fw fa-dashboard"></i><span>Dashboard</span></a>
            <a href="#" id="panelRef"><i class="fa fa-fw fa-envelope-o"></i><span>Messages</span></a>
            <a href="#" id="sponsorsLink"><i class="fa fa-fw fa-cogs"></i><span>Profile</span></a>
            <a href="/logout" id="aboutLink"><i class="fa fa-fw fa-sign-out"></i><span>Logout</span></a>
          </div>
        </nav>
        <button id="close-button" class="close-button">Close menu</button>
      </div>
      <header>
        <a href="#" id="open-button" class="white-color"><i class="fa fa-bars"></i></a>
        <div class="right">
          <a href="#" class="white-color notifications-container" id="notifications"><span class="notifications bg-prm-color">4</span> <i class="fa fa-bell-o"></i></a>
        </div>
        <form class="search-form" id="search-form">
          <div class="input with-icon">
            <i class="fa fa-search"></i>
            <input id="search-input" type="text" placeholder="find people">
          </div>
          <button class="hide" type="submit"></button>
        </form>
      </header>
    </section>
    <section class="bg-grey">
      <div class="top-bar">
        <div class="select-bar">
          <select name="selector" id="event-selector">
            <option value="popular">Most Popular</option>
            <option value="latest">Latest</option>
          </select>
          <i class="fa fa-angle-down"></i>
        </div>
        <div class="choices right" id="option-list">
          <a href="#" class="active" data-option="events">Events</a>
          <span>/</span>
          <a href="#" data-option="users">Attendees</a>
        </div>
      </div>
      <div class="cards clearfix">
        <div class="col-xs-6">
          <a href="/events/1">
            <article class="card">
              <div class="eventLogo">
                <h2>AngelHack Athens</h2>
                <h3>June 6-7, 2015</h3>
              </div>
              <div class="card-details">
                <p>233 participants</p>
              </div>
            </article>
          </a>
        </div>
        <div class="col-xs-6">
          <a href="/events/1">
            <article class="card">
              <div class="eventLogo">
                <h2>AngelHack Athens</h2>
                <h3>June 6-7, 2015</h3>
              </div>
              <div class="card-details">
                <p>233 participants</p>
              </div>
            </article>
          </a>
        </div>
        <div class="col-xs-6">
          <a href="/events/1">
            <article class="card">
              <div class="eventLogo">
                <h2>AngelHack Athens</h2>
                <h3>June 6-7, 2015</h3>
              </div>
              <div class="card-details">
                <p>233 participants</p>
              </div>
            </article>
          </a>
        </div>
        <div class="col-xs-6">
          <a href="/events/1">
            <article class="card">
              <div class="eventLogo">
                <h2>AngelHack Athens</h2>
                <h3>June 6-7, 2015</h3>
              </div>
              <div class="card-details">
                <p>233 participants</p>
              </div>
            </article>
          </a>
        </div>
      </div>
    </section>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="/js/libs/crossvent.min.js"></script>
    <script src="/js/dashboard.js"></script>
  </body>

</html>
